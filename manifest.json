{
    "manifest_version": 3,
    "name": "Draft Ranking Adjustments",
    "description": "Will portray how much higher or lower the draft site has the player ranked.",
    "version": "1.0",
    "action": {
      "default_title": "Say It",
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["*://sleeper.com/draft/*"],
          "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [
      {
        "resources": [ "data/*.json" ],
        "matches": [ "<all_urls>" ]
      }
    ],
    "permissions": [
      "activeTab",
      "storage"
    ]
}